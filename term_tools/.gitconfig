[core]
  exludeFile = ~/.global_gitignore        
  pager = "delta"
[pull]
  rebase = false
[push]
  autoSetupRemote = true
[delta]  
  navigate = true
[interactive]
  diffFilter = "delta --color-only"
[merge]
  conflictstyle = "diff3"
[diff]
  colorMoved = "default"
[init]
  defaultBranch = "main"
